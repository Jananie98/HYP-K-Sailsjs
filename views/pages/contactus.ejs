<div id="contactus" v-cloak>

     <!-- ######################################################## Form area ##############################################################-->
     <section>
      <div class="container-fluid bg-light">
          <div class="container p-5">
              <h2 class="text-center" data-aos="fade-up">Contact Us</h2>
              <form>
                  <div class="row">
                      <!-- Map -->
                      <div class="col-md-6">
                          <div class="ratio ratio-4x3">
                              <iframe class="w-100" height="500" id="gmap_canvas" src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d15843.041692476974!2d79.8590799!3d6.9192193!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x833392042e54c642!2sOn320%20Residencies!5e0!3m2!1sen!2slk!4v1603885049886!5m2!1sen!2slk" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe>
                              <!-- <iframe src="https://www.google.com/maps/place/On320+Residencies/@6.919219,79.85908,14z/data=!4m8!3m7!1s0x0:0x833392042e54c642!5m2!4m1!1i2!8m2!3d6.9192193!4d79.8590799?hl=en" allowfullscreen></iframe> -->
                          </div>
                      </div>
                      <!-- Contact Us form -->
                      <div class="col-md-6">
                          <label class="form-label">Name <span class="text-danger">*</span> </label>
                          <input class="form-control mb-3" type="text" placeholder="Enter name" required>

                          <label class="form-label">Email <span class="text-danger">*</span> </label>
                          <input class="form-control mb-3" type="text" placeholder="Enter email" required>

                          <label class="form-label">Company Name <span class="text-danger">*</span> </label>
                          <input class="form-control mb-3" type="text" placeholder="Enter company name" required>

                          <label class="form-label">Name </label>
                          <select class="form-control mb-3">
                              <option selected>Select Service</option>
                              <option>Trading</option>
                              <option>Leisure and Tours</option>
                              <option>Partnerships</option>
                              <option>Retail</option>
                          </select>

                          <label class="form-label">Message <span class="text-danger">*</span> </label>
                          <textarea class="form-control mb-3" type="text" placeholder="Enter Message" required></textarea>

                          <label class="form-label">Captcha <span class="text-danger">*</span> </label>
                          <div class="g-recaptcha mb-3" data-sitekey="6Ldbdg0TAAAAAI7KAf72Q6uagbWzWecTeBWmrCpJ"></div>
                      </div>
                  </div>   
              </form>
          </div>
      </div>
  </section>

</div>
<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
